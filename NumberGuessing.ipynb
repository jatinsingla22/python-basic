   "source": [
    "import random\n",
    "number = random.randint(1,100)\n",
    "guess = 0\n",
    "while guess != number:\n",
    "    guess = int(input(\"Enter Guess: \"))\n",
    "    if (guess<number): \n",
    "        print(\"Guess higher!\")\n",
    "    elif (guess>number): \n",
    "        print(\"Guess lower!\")\n",
    "    else: \n",
    "        print(\"You won!\")"
